<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_fragm_datosPersonales"
    tools:context=".ui.datos_personales.DatosPersonalesFragment">


    <Button
        android:id="@+id/vermisdatos"
        android:layout_width="233dp"
        android:layout_height="88dp"
        android:background="@drawable/boton_redondo"
        android:text="@string/ver_mis_datos_actuales"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.252" />

    <Button
        android:id="@+id/actualizarmisdatos"
        android:layout_width="233dp"
        android:layout_height="88dp"
        android:layout_marginTop="108dp"
        android:background="@drawable/boton_redondo"
        android:text="@string/actualizar_mis_datos"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vermisdatos" />
</androidx.constraintlayout.widget.ConstraintLayout>